{
  "name": "running-cat",
  "displayName": "Running Cat",
  "description": "A fun extension that shows a cat running between your code lines",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "running-cat.toggleCat",
        "title": "Toggle Running Cat"
      },
      {
        "command": "running-cat.startCat",
        "title": "Start Running Cat"
      },
      {
        "command": "running-cat.stopCat",
        "title": "Stop Running Cat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.x",
    "@types/vscode": "^1.107.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.52.0",
    "typescript": "^5.9.3"
  }
}
